<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.alice.crm.settings.dao.DeptDao" >

    <select id="selectDepts" resultType="com.alice.crm.settings.entity.Dept">
        SELECT
        *
        FROM
        t_dept
        ORDER BY deptId desc
        limit 0,10
    </select>

</mapper>